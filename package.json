{
  "name": "postinfo_connect",
  "version": "1.0.0",
  "description": "Postinfo CRM",
  "main": "server/index.js",
  "scripts": {
    "test": "mocha",
    "bundle": "browserify client/ -t babelify -o server/public/main.js",
    "bundle:watch": "watchify client/ -vd -t babelify -o server/public/main.js",
    "start": "node .",
    "start:watch": "nodemon --ignore cookies/*.json --ignore server/cookies/*.json .",
    "watch": "run-p bundle:watch start:watch",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyaaan/postinfo_connect.git"
  },
  "keywords": [
    "CRM",
    "postinfo",
    "marketing",
    "sales",
    "email",
    "campaign",
    "drip"
  ],
  "author": "John Yamashiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jyaaan/postinfo_connect/issues"
  },
  "homepage": "https://github.com/jyaaan/postinfo_connect#readme",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "csvtojson": "^1.1.12",
    "express": "^4.16.3",
    "fast-csv": "^2.4.1",
    "knex": "^0.13.0",
    "mocha": "^3.5.3",
    "node-schedule": "^1.3.0",
    "node-uuid": "^1.4.8",
    "npm": "^5.8.0",
    "pg": "^6.4.2",
    "pg-format": "^1.0.4",
    "pug": "^2.0.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "redux": "^3.7.2",
    "request": "^2.85.0",
    "selenium-webdriver": "^3.6.0",
    "semantic-ui-react": "^0.70.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^2.1.0",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "watchify": "^3.11.0"
  }
}
